{
    "name": "@clytage/eslint-config",
    "version": "2.0.0",
    "description": "An ESLint shareable configuration that we used for our projects.",
    "main": "index.js",
    "scripts": {
        "lint": "eslint --ignore-path .gitignore .",
        "lint:fix": "eslint --ignore-path .gitignore . --fix",
        "release": "pnpm publish --access public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/clytage/eslint-config.git"
    },
    "keywords": [
        "eslint",
        "eslintconfig",
        "eslint-config",
        "lint",
        "linter",
        "tslint",
        "typescript",
        "typescript-eslint"
    ],
    "author": "Clytage <admin@clytage.org>",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/clytage/eslint-config/issues"
    },
    "homepage": "https://github.com/clytage/eslint-config#readme",
    "eslintConfig": {
        "extends": "./index.js"
    },
    "devDependencies": {
        "@eslint/js": "^8.44.0",
        "@types/node": "*",
        "@typescript-eslint/eslint-plugin": ">=6.0.0",
        "@typescript-eslint/parser": ">=6.0.0",
        "eslint": ">=8.44.0",
        "typescript": ">=3.3.1 <6.0.0"
    },
    "peerDependencies": {
        "@eslint/js": "^8.44.0",
        "@typescript-eslint/eslint-plugin": ">=6.0.0",
        "@typescript-eslint/parser": ">=6.0.0",
        "eslint": ">=8.44.0",
        "typescript": ">=3.3.1 <6.0.0"
    },
    "peerDependenciesMeta": {
        "@typescript-eslint/eslint-plugin": {
            "optional": true
        },
        "@typescript-eslint/parser": {
            "optional": true
        },
        "typescript": {
            "optional": true
        }
    }
}
